    "cloudinary": "^1.18.1",
    "cloudinary-react": "^1.3.0",
    "connect-mongo": "^3.2.0",
    "cors": "^2.8.5",
    "dom-to-image-more": "^2.8.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "file-saver": "^2.0.2",
    "helmet": "^3.21.2",
    "if-env": "^1.0.4",
    "jquery": "^3.4.1",
    "mongoose": "^5.8.2",
    "morgan": "^1.9.1",
    "multer": "^1.4.2",
    "node-sass": "^7.0.0",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
